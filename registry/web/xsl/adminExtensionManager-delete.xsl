<?xml version="1.0" encoding='UTF-8'?> <xsl:stylesheet version="1.0"  xmlns:xsl="http://www.w3.org/1999/XSL/Transform"   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"  xmlns:dc="http://purl.org/dc/elements/1.1/"  xmlns:dcq="http://purl.org/dc/terms/"  xmlns:res="http://dublincore.org/registry/resources/"  xmlns:java="java" ><xsl:output method="xml" indent="yes" encoding="UTF-8"    doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"    doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN" /> <xsl:include href="banner.xsl"/><xsl:include href="footer.xsl"/>	<xsl:include href="_url_encode.xsl"/><xsl:param name="reqType" select="''"/><xsl:param name="uiStyle" select="'std'"/><xsl:param name="uiLang" select="'en-US'"/><xsl:param name="resultsLang" select="'en-US'"/><xsl:param name="item" select="''"/><xsl:param name="type" select="''"/><xsl:param name="seqno" select="''"/><xsl:param name="label" select="''"/><xsl:param name="requires" select="''"/><xsl:param name="location" select="'administration'"/><xsl:variable name="rdf" select="'http://www.w3.org/1999/02/22-rdf-syntax-ns#'" /><xsl:variable name="rdfs" select="'http://www.w3.org/2000/01/rdf-schema#'"/><xsl:variable name="dc" select="'http://purl.org/dc/elements/1.1/'" /><xsl:variable name="dcq" select="'http://purl.org/dc/terms/'" /><xsl:variable name="dctype" select="'http://purl.org/dc/dcmitype/'" /><xsl:variable name="locale" select="java:util.Locale.new(substring($uiLang,1,2), substring($uiLang,4,2))"/><xsl:variable name="resources" select="java:util.ResourceBundle.getBundle('ui', $locale)"/><xsl:template match="rdf:RDF">	<html>	<head>		<title>The Open Metadata Registry</title>		<link type="text/css" rel="stylesheet" href="/dcregistry/css/default.css" />	</head>	<body>	<xsl:call-template name="banner">				<xsl:with-param name="location" select="$location" />	</xsl:call-template>	<p/>	<table width="100%" cellspacing="0" cellpadding="0" border="1">	<tr><td>		<table width="100%" cellspacing="3" cellpadding="0" border="0">	<tr><td>		<table width="100%" cellspacing="0" cellpadding="3" border="0">		<tr><td width="25%" valign="top" bgcolor="#EEEEEE">	Use this form to delete an extension.	<p/>	Press the 'Confirm Delete' button to delete the extension, or the 'Cancel' button to cancel this action.	</td><td width="75%" bgcolor="FFFFFF" valign="top">		<xsl:apply-templates select ="rdf:Description" />	</td></tr>	</table>		</td></tr>	</table>		</td></tr>	</table>		<xsl:variable name="encoded_item">		<xsl:value-of select="java:net.URLEncoder.encode($item,'UTF8')" />	</xsl:variable>		<p/>	[<a href="/dcregistry/adminServlet">Back to Top Page</a>]	<p/><xsl:call-template name="footer" />	</body>	</html></xsl:template><xsl:template match="rdf:Description">	<xsl:variable name="res">		<xsl:value-of select="./@rdf:about"/>	</xsl:variable>	<form action="/dcregistry/adminExtensionManagerServlet" method="post">	<table width="100%" border="0" cellspacing="2" cellpadding="2">	<tr><td colspan="3"><h4>Confim delete of the following browse function?</h4></td></tr>	<tr><td width="5%">&#160;</td>	<td width="20%">	<b>Type:</b>	</td><td width="75%">	<xsl:value-of select="$type"/>	</td></tr>	<tr><td width="5%">&#160;	</td><td width="20%">	<b>Sort Order:</b>	</td><td width="75%">	<xsl:value-of select="$seqno"/>	</td></tr>		<tr><td width="5%">&#160;	</td><td width="20%">	<b>Property Value:</b>	</td><td width="75%">	<xsl:value-of select="$label"/>	</td></tr>			<tr><td width="5%">&#160;	</td><td width="20%">	<b>Class Name:</b>	</td><td width="75%">	<xsl:value-of select="$requires"/>	</td></tr>			<tr><td width="15%" colspan="2">&#160;</td>	<td width="85%">	<input name="reqType" value="Confirm Delete" type="submit" />&#160;&#160;<input name="reqType" value="Cancel" type="submit" />	</td></tr>	<input type="hidden" name="item" value="{$item}" />	</table>	</form>	</xsl:template></xsl:stylesheet>