<rdf:Property rdf:ID = "type">
  <rdfs:label>Resource Type</rdfs:label>
  <rdfs:comment>The nature or genre of the content of the resource.</rdfs:comment>
  <rdfs:isDefinedBy rdf:resource = "http://rdf.dev.oclc.org/eor/1999/12/22-dcv" />
  <rdfs:range rdf:resource = "ResourceType" />
</rdf:Property>

